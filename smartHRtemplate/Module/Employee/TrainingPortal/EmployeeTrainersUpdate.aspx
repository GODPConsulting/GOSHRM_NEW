            </td>
            <td class="style9">
                <asp:Label ID="lblsession" runat="server" Font-Size="12px" Font-Names="Verdana"
                    Font-Bold="False" ForeColor="#666666"></asp:Label>
            </td>
        </tr>
        <tr>
            <td valign="top" class="style5">
                <asp:Label ID="lbldateassessment5" runat="server" Font-Size="12px" Font-Names="Verdana"
                    Font-Bold="True" ForeColor="#666666">Date</asp:Label>
            </td>
            <td class="style9">
                <asp:Label ID="lbldate" runat="server" Font-Size="12px" Font-Names="Verdana"
                    Font-Bold="False" ForeColor="#666666"></asp:Label>
            </td>
        </tr>
        <tr>
            <td valign="top" class="style5">
                <asp:Label ID="lbldateassessment6" runat="server" Font-Size="12px" Font-Names="Verdana"
                    Font-Bold="True" ForeColor="#666666">Time</asp:Label>
            </td>
            <td class="style9">
                <asp:Label ID="lbltime" runat="server" Font-Size="12px" Font-Names="Verdana"
                    Font-Bold="False" ForeColor="#666666"></asp:Label>
            </td>
        </tr>
        <tr>
            <td valign="top" class="style5">
                <asp:Label ID="lbldateassessment7" runat="server" Font-Size="12px" Font-Names="Verdana"
                    Font-Bold="True" ForeColor="#666666">Venue</asp:Label>
            </td>
            <td class="style9">
                <asp:Label ID="lblvenue" runat="server" Font-Size="12px" Font-Names="Verdana"
                    Font-Bold="False" ForeColor="#666666"></asp:Label>
            </td>
        </tr>
         <tr>
            <td class="style5" valign="top">
                <asp:Label ID="Label1" runat="server" Font-Size="12px" Font-Names="Verdana"
                    Font-Bold="True" ForeColor="#666666">Objectives</asp:Label>
            </td>
            <td class="style9">
             <asp:TextBox ID="txtObjective" runat="server" Font-Names="Verdana" Font-Size="12px"
